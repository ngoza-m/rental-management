<div class="container mt-3">
    <p><%#= link_to 'Logout', destroy_user_session_path, data: { turbo_method: :delete } %></p>
    <p> <%#= link_to 'View Dashboard', dashboard_path%>

    <% @properties.each do |property| %>
        <h2> <%= property.name %> </h2>
        <p> <%= property.address %> </p>
        <p> <%= property.description %> </p>
        <p> Rent: ZK<%= property.rent %> </p>
        <p> View this property <%= link_to 'View', property_path(property) %></p>
    <% end %>
</div>